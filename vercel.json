{
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/index.html"
		}
	],
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/locales/(.*)",
			"dest": "/public/locales/$1"
		},
		{
			"src": "/public/(.*)",
			"dest": "/public/$1"
		},
		{
			"src": "/menu",
			"dest": "/index.html"
		}
	]
}
